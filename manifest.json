  {
    "name": "Amazon Switch Bot",
    "version": "1.0",
    "description": "lmao we're doing this",
    "manifest_version": 2,
    "permissions": [
      "declarativeContent",
      "storage",
      "activeTab",
      "tabs",
      "notifications",
      "http://*/",
      "https://*/"
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
  }